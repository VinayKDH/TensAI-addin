# TENS AI Office Add-in Environment Configuration

# Server Configuration
PORT=3000
NODE_ENV=development

# TENS AI API Configuration
TENS_AI_API_BASE_URL=https://api.dev2.tens-ai.com
TENS_AI_API_TIMEOUT=30000

# Security Configuration
CORS_ORIGINS=https://localhost:3000,https://127.0.0.1:3000,https://www.dev2.tens-ai.com

# SSL Configuration (for production)
SSL_CERT_PATH=
SSL_KEY_PATH=

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
CACHE_TTL=3600
REDIS_URL=

# Database Configuration (if needed for user management)
DATABASE_URL=
DATABASE_POOL_SIZE=10

# Monitoring and Analytics
ANALYTICS_ENABLED=false
ANALYTICS_KEY=

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_USER_MANAGEMENT=false
ENABLE_ADVANCED_FEATURES=true
